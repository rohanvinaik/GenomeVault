from typing import Any, Dict

import traceback
import sys
from io import StringIO


class Log:
    def __init__(self) -> None:
            """TODO: Add docstring for __init__"""
    self._contents = []

    def add_content(self, *content) -> None:
            """TODO: Add docstring for add_content"""
    self._contents.append(" ".join(content))

    def add_exception(self) -> None:
            """TODO: Add docstring for add_exception"""
    s = StringIO()
        exc_info = sys.exc_info()
        traceback.print_exception(exc_info[0], exc_info[1], exc_info[2], limit=None, file=s)
        self._contents.append(s.getvalue())

    def get_contents(self) -> Any:
            """TODO: Add docstring for get_contents"""
    return "\n".join(self._contents)

    def clear_log(self) -> None:
            """TODO: Add docstring for clear_log"""
    del self._contents[:]
