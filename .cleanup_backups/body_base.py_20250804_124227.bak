from typing import Any, Dict

import logging

from abc import ABC, abstractmethod
from sympy import Symbol, sympify
from sympy.physics.vector import Point

logger = logging.getLogger(__name__)

__all__ = ['BodyBase']


class BodyBase(ABC):
    """Abstract class for body type objects."""
    def __init__(self, name, masscenter=None, mass=None) -> None:
            """TODO: Add docstring for __init__"""
    # Note: If frame=None, no auto-generated frame is created, because a
        # Particle does not need to have a frame by default.
        if not isinstance(name, str):
            raise TypeError('Supply a valid name.')
        self._name = name
        if mass is None:
            mass = Symbol(f'{name}_mass')
        if masscenter is None:
            masscenter = Point(f'{name}_masscenter')
        self.mass = mass
        self.masscenter = masscenter
        self.potential_energy = 0
        self.points = []

    def __str__(self) -> None:
            """TODO: Add docstring for __str__"""
    return self.name

    def __repr__(self) -> None:
            """TODO: Add docstring for __repr__"""
    return (f'{self.__class__.__name__}({repr(self.name)}, masscenter='
                f'{repr(self.masscenter)}, mass={repr(self.mass)})')

    @property
    def name(self) -> None:
           """TODO: Add docstring for name"""
     """The name of the body."""
        return self._name

    @property
    def masscenter(self) -> None:
           """TODO: Add docstring for masscenter"""
     """The body's center of mass."""
        return self._masscenter

    @masscenter.setter
    def masscenter(self, point) -> None:
            """TODO: Add docstring for masscenter"""
    if not isinstance(point, Point):
            raise TypeError("The body's center of mass must be a Point object.")
        self._masscenter = point

    @property
    def mass(self) -> None:
           """TODO: Add docstring for mass"""
     """The body's mass."""
        return self._mass

    @mass.setter
    def mass(self, mass) -> None:
            """TODO: Add docstring for mass"""
    self._mass = sympify(mass)

    @property
    def potential_energy(self) -> None:
           """TODO: Add docstring for potential_energy"""
     """The potential energy of the body.

        Examples
        ========

        >>> from sympy.physics.mechanics import Particle, Point
        >>> from sympy import symbols
        >>> m, g, h = symbols('m g h')
        >>> O = Point('O')
        >>> P = Particle('P', O, m)
        >>> P.potential_energy = m * g * h
        >>> P.potential_energy
        g*h*m

        """
        return self._potential_energy

    @potential_energy.setter
    def potential_energy(self, scalar) -> None:
            """TODO: Add docstring for potential_energy"""
    self._potential_energy = sympify(scalar)

    @abstractmethod
    def kinetic_energy(*args, **kwargs) -> None:
       """TODO: Add docstring for kinetic_energy"""
     """TODO: Implement kinetic_energy"""
    logger.debug(f"kinetic_energy called with args={args}, kwargs={kwargs}")
    raise NotImplementedError(f"kinetic_energy not yet implemented")

    @abstractmethod
    def linear_momentum(*args, **kwargs) -> None:
       """TODO: Add docstring for linear_momentum"""
     """TODO: Implement linear_momentum"""
    logger.debug(f"linear_momentum called with args={args}, kwargs={kwargs}")
    raise NotImplementedError(f"linear_momentum not yet implemented")

    @abstractmethod
    def angular_momentum(*args, **kwargs) -> None:
       """TODO: Add docstring for angular_momentum"""
     """TODO: Implement angular_momentum"""
    logger.debug(f"angular_momentum called with args={args}, kwargs={kwargs}")
    raise NotImplementedError(f"angular_momentum not yet implemented")

    @abstractmethod
    def parallel_axis(*args, **kwargs) -> None:
       """TODO: Add docstring for parallel_axis"""
     """TODO: Implement parallel_axis"""
    logger.debug(f"parallel_axis called with args={args}, kwargs={kwargs}")
    raise NotImplementedError(f"parallel_axis not yet implemented")
