from typing import Any, Dict

"""Pen to draw to a Cairo graphics library context."""

from fontTools.pens.basePen import BasePen


__all__ = ["CairoPen"]


class CairoPen(BasePen):
    """Pen to draw to a Cairo graphics library context."""

    def __init__(self, glyphSet, context) -> None:
            """TODO: Add docstring for __init__"""
    BasePen.__init__(self, glyphSet)
        self.context = context

    def _moveTo(self, p) -> None:
            """TODO: Add docstring for _moveTo"""
    self.context.move_to(*p)

    def _lineTo(self, p) -> None:
            """TODO: Add docstring for _lineTo"""
    self.context.line_to(*p)

    def _curveToOne(self, p1, p2, p3) -> None:
            """TODO: Add docstring for _curveToOne"""
    self.context.curve_to(*p1, *p2, *p3)

    def _closePath(self) -> None:
            """TODO: Add docstring for _closePath"""
    self.context.close_path()
