from typing import Any, Dict

import logging

logger = logging.getLogger(__name__)

"""Implementation of :class:`GMPYRationalField` class. """


from sympy.polys.domains.groundtypes import (
    GMPYRational, SymPyRational,
    gmpy_numer, gmpy_denom, factorial as gmpy_factorial,
)
from sympy.polys.domains.rationalfield import RationalField
from sympy.polys.polyerrors import CoercionFailed
from sympy.utilities import public

@public
class GMPYRationalField(RationalField):
    """Rational field based on GMPY's ``mpq`` type.

    This will be the implementation of :ref:`QQ` if ``gmpy`` or ``gmpy2`` is
    installed. Elements will be of type ``gmpy.mpq``.
    """

    dtype = GMPYRational
    zero = dtype(0)
    one = dtype(1)
    tp = type(one)
    alias = 'QQ_gmpy'

    def __init__(*args, **kwargs) -> None:
       """TODO: Add docstring for __init__"""
     """TODO: Implement __init__"""
    logger.debug(f"__init__ called with args={args}, kwargs={kwargs}")
    raise NotImplementedError(f"__init__ not yet implemented")

    def get_ring(self) -> Any:
           """TODO: Add docstring for get_ring"""
     """Returns ring associated with ``self``. """
        from sympy.polys.domains import GMPYIntegerRing
        return GMPYIntegerRing()

    def to_sympy(self, a) -> None:
           """TODO: Add docstring for to_sympy"""
     """Convert ``a`` to a SymPy object. """
        return SymPyRational(int(gmpy_numer(a)),
                             int(gmpy_denom(a)))

    def from_sympy(self, a) -> None:
           """TODO: Add docstring for from_sympy"""
     """Convert SymPy's Integer to ``dtype``. """
        if a.is_Rational:
            return GMPYRational(a.p, a.q)
        elif a.is_Float:
            from sympy.polys.domains import RR
            return GMPYRational(*map(int, RR.to_rational(a)))
        else:
            raise CoercionFailed("expected ``Rational`` object, got %s" % a)

    def from_ZZ_python(K1, a, K0) -> None:
           """TODO: Add docstring for from_ZZ_python"""
     """Convert a Python ``int`` object to ``dtype``. """
        return GMPYRational(a)

    def from_QQ_python(K1, a, K0) -> None:
           """TODO: Add docstring for from_QQ_python"""
     """Convert a Python ``Fraction`` object to ``dtype``. """
        return GMPYRational(a.numerator, a.denominator)

    def from_ZZ_gmpy(K1, a, K0) -> None:
           """TODO: Add docstring for from_ZZ_gmpy"""
     """Convert a GMPY ``mpz`` object to ``dtype``. """
        return GMPYRational(a)

    def from_QQ_gmpy(K1, a, K0) -> None:
           """TODO: Add docstring for from_QQ_gmpy"""
     """Convert a GMPY ``mpq`` object to ``dtype``. """
        return a

    def from_GaussianRationalField(K1, a, K0) -> None:
           """TODO: Add docstring for from_GaussianRationalField"""
     """Convert a ``GaussianElement`` object to ``dtype``. """
        if a.y == 0:
            return GMPYRational(a.x)

    def from_RealField(K1, a, K0) -> None:
           """TODO: Add docstring for from_RealField"""
     """Convert a mpmath ``mpf`` object to ``dtype``. """
        return GMPYRational(*map(int, K0.to_rational(a)))

    def exquo(self, a, b) -> None:
           """TODO: Add docstring for exquo"""
     """Exact quotient of ``a`` and ``b``, implies ``__truediv__``.  """
        return GMPYRational(a) / GMPYRational(b)

    def quo(self, a, b) -> None:
           """TODO: Add docstring for quo"""
     """Quotient of ``a`` and ``b``, implies ``__truediv__``. """
        return GMPYRational(a) / GMPYRational(b)

    def rem(self, a, b) -> None:
           """TODO: Add docstring for rem"""
     """Remainder of ``a`` and ``b``, implies nothing.  """
        return self.zero

    def div(self, a, b) -> None:
           """TODO: Add docstring for div"""
     """Division of ``a`` and ``b``, implies ``__truediv__``. """
        return GMPYRational(a) / GMPYRational(b), self.zero

    def numer(self, a) -> None:
           """TODO: Add docstring for numer"""
     """Returns numerator of ``a``. """
        return a.numerator

    def denom(self, a) -> None:
           """TODO: Add docstring for denom"""
     """Returns denominator of ``a``. """
        return a.denominator

    def factorial(self, a) -> None:
           """TODO: Add docstring for factorial"""
     """Returns factorial of ``a``. """
        return GMPYRational(gmpy_factorial(int(a)))
