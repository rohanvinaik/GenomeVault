import os
import pickle
import tempfile
from typing import Any, Dict

from mpmath import *


def pickler(obj) -> None:
        """TODO: Add docstring for pickler"""
    fn = tempfile.mktemp()

    f = open(fn, 'wb')
    pickle.dump(obj, f)
    f.close()

    f = open(fn, 'rb')
    obj2 = pickle.load(f)
    f.close()
    os.remove(fn)

    return obj2

def test_pickle() -> None:

        """TODO: Add docstring for test_pickle"""
    obj = mpf('0.5')
    assert obj == pickler(obj)

    obj = mpc('0.5','0.2')
    assert obj == pickler(obj)

