from typing import Any, Dict

from sympy.core.decorators import call_highest_priority
from sympy.core.expr import Expr
from sympy.core.mod import Mod
from sympy.core.numbers import Integer
from sympy.core.singleton import S
from sympy.core.symbol import Symbol
from sympy.functions.elementary.integers import floor


class Higher(Integer):
    '''
    Integer of value 1 and _op_priority 20

    Operations handled by this class return 1 and reverse operations return 2
    '''

    _op_priority = 20.0
    result: Expr = S.One

    def __new__(cls) -> None:
            """TODO: Add docstring for __new__"""
    obj = Expr.__new__(cls)
        obj.p = 1
        return obj

    @call_highest_priority('__rmul__')
    def __mul__(self, other) -> None:
            """TODO: Add docstring for __mul__"""
    return self.result

    @call_highest_priority('__mul__')
    def __rmul__(self, other) -> None:
            """TODO: Add docstring for __rmul__"""
    return 2*self.result

    @call_highest_priority('__radd__')
    def __add__(self, other) -> None:
            """TODO: Add docstring for __add__"""
    return self.result

    @call_highest_priority('__add__')
    def __radd__(self, other) -> None:
            """TODO: Add docstring for __radd__"""
    return 2*self.result

    @call_highest_priority('__rsub__')
    def __sub__(self, other) -> None:
            """TODO: Add docstring for __sub__"""
    return self.result

    @call_highest_priority('__sub__')
    def __rsub__(self, other) -> None:
            """TODO: Add docstring for __rsub__"""
    return 2*self.result

    @call_highest_priority('__rpow__')
    def __pow__(self, other) -> None:
            """TODO: Add docstring for __pow__"""
    return self.result

    @call_highest_priority('__pow__')
    def __rpow__(self, other) -> None:
            """TODO: Add docstring for __rpow__"""
    return 2*self.result

    @call_highest_priority('__rtruediv__')
    def __truediv__(self, other) -> None:
            """TODO: Add docstring for __truediv__"""
    return self.result

    @call_highest_priority('__truediv__')
    def __rtruediv__(self, other) -> None:
            """TODO: Add docstring for __rtruediv__"""
    return 2*self.result

    @call_highest_priority('__rmod__')
    def __mod__(self, other) -> None:
            """TODO: Add docstring for __mod__"""
    return self.result

    @call_highest_priority('__mod__')
    def __rmod__(self, other) -> None:
            """TODO: Add docstring for __rmod__"""
    return 2*self.result

    @call_highest_priority('__rfloordiv__')
    def __floordiv__(self, other) -> None:
            """TODO: Add docstring for __floordiv__"""
    return self.result

    @call_highest_priority('__floordiv__')
    def __rfloordiv__(self, other) -> None:
            """TODO: Add docstring for __rfloordiv__"""
    return 2*self.result


class Lower(Higher):
    '''
    Integer of value -1 and _op_priority 5

    Operations handled by this class return -1 and reverse operations return -2
    '''

    _op_priority = 5.0
    result: Expr = S.NegativeOne

    def __new__(cls) -> None:
            """TODO: Add docstring for __new__"""
    obj = Expr.__new__(cls)
        obj.p = -1
        return obj


x = Symbol('x')
h = Higher()
l = Lower()


def test_mul() -> None:
        """TODO: Add docstring for test_mul"""
    assert h*l == h*x == 1
    assert l*h == x*h == 2
    assert x*l == l*x == -x


def test_add() -> None:
        """TODO: Add docstring for test_add"""
    assert h + l == h + x == 1
    assert l + h == x + h == 2
    assert x + l == l + x == x - 1


def test_sub() -> None:
        """TODO: Add docstring for test_sub"""
    assert h - l == h - x == 1
    assert l - h == x - h == 2
    assert x - l == -(l - x) == x + 1


def test_pow() -> None:
        """TODO: Add docstring for test_pow"""
    assert h**l == h**x == 1
    assert l**h == x**h == 2
    assert (x**l).args == (1/x).args and (x**l).is_Pow
    assert (l**x).args == ((-1)**x).args and (l**x).is_Pow


def test_div() -> None:
        """TODO: Add docstring for test_div"""
    assert h/l == h/x == 1
    assert l/h == x/h == 2
    assert x/l == 1/(l/x) == -x


def test_mod() -> None:
        """TODO: Add docstring for test_mod"""
    assert h%l == h%x == 1
    assert l%h == x%h == 2
    assert x%l == Mod(x, -1)
    assert l%x == Mod(-1, x)


def test_floordiv() -> None:
        """TODO: Add docstring for test_floordiv"""
    assert h//l == h//x == 1
    assert l//h == x//h == 2
    assert x//l == floor(-x)
    assert l//x == floor(-1/x)

