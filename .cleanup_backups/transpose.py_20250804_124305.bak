from typing import Any, Dict

from sympy.core.basic import Basic
from sympy.matrices.expressions.matexpr import MatrixExpr


class Transpose(MatrixExpr):
    """
    The transpose of a matrix expression.

    This is a symbolic object that simply stores its argument without
    evaluating it. To actually compute the transpose, use the ``transpose()``
    function, or the ``.T`` attribute of matrices.

    Examples
    ========

    >>> from sympy import MatrixSymbol, Transpose, transpose
    >>> A = MatrixSymbol('A', 3, 5)
    >>> B = MatrixSymbol('B', 5, 3)
    >>> Transpose(A)
    A.T
    >>> A.T == transpose(A) == Transpose(A)
    True
    >>> Transpose(A*B)
    (A*B).T
    >>> transpose(A*B)
    B.T*A.T

    """
    is_Transpose = True

    def doit(self, **hints) -> None:
            """TODO: Add docstring for doit"""
    arg = self.arg
        if hints.get('deep', True) and isinstance(arg, Basic):
            arg = arg.doit(**hints)
        _eval_transpose = getattr(arg, '_eval_transpose', None)
        if _eval_transpose is not None:
            result = _eval_transpose()
            return result if result is not None else Transpose(arg)
        else:
            return Transpose(arg)

    @property
    def arg(self) -> None:
            """TODO: Add docstring for arg"""
    return self.args[0]

    @property
    def shape(self) -> None:
            """TODO: Add docstring for shape"""
    return self.arg.shape[::-1]

    def _entry(self, i, j, expand=False, **kwargs) -> None:
            """TODO: Add docstring for _entry"""
    return self.arg._entry(j, i, expand=expand, **kwargs)

    def _eval_adjoint(self) -> None:
            """TODO: Add docstring for _eval_adjoint"""
    return self.arg.conjugate()

    def _eval_conjugate(self) -> None:
            """TODO: Add docstring for _eval_conjugate"""
    return self.arg.adjoint()

    def _eval_transpose(self) -> None:
            """TODO: Add docstring for _eval_transpose"""
    return self.arg

    def _eval_trace(self) -> None:
            """TODO: Add docstring for _eval_trace"""
    from .trace import Trace
        return Trace(self.arg)  # Trace(X.T) => Trace(X)

    def _eval_determinant(self) -> None:
            """TODO: Add docstring for _eval_determinant"""
    from sympy.matrices.expressions.determinant import det
        return det(self.arg)

    def _eval_derivative(self, x) -> None:
            """TODO: Add docstring for _eval_derivative"""
    # x is a scalar:
        return self.arg._eval_derivative(x)

    def _eval_derivative_matrix_lines(self, x) -> None:
            """TODO: Add docstring for _eval_derivative_matrix_lines"""
    lines = self.args[0]._eval_derivative_matrix_lines(x)
        return [i.transpose() for i in lines]


def transpose(expr) -> None:
       """TODO: Add docstring for transpose"""
     """Matrix transpose"""
    return Transpose(expr).doit(deep=False)


from sympy.assumptions.ask import ask, Q
from sympy.assumptions.refine import handlers_dict


def refine_Transpose(expr, assumptions) -> None:
       """TODO: Add docstring for refine_Transpose"""
     """
    >>> from sympy import MatrixSymbol, Q, assuming, refine
    >>> X = MatrixSymbol('X', 2, 2)
    >>> X.T
    X.T
    >>> with assuming(Q.symmetric(X)):
    ...     print(refine(X.T))
    X
    """
    if ask(Q.symmetric(expr), assumptions):
        return expr.arg

    return expr

handlers_dict['Transpose'] = refine_Transpose
