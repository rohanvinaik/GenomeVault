from typing import Any, Dict

def _makeunicodes(f) -> None:
        """TODO: Add docstring for _makeunicodes"""
    lines = iter(f.readlines())
    unicodes = {}
    for line in lines:
        if not line:
            continue
        num, name = line.split(";")[:2]
        if name[0] == "<":
            continue  # "<control>", etc.
        num = int(num, 16)
        unicodes[num] = name
    return unicodes


class _UnicodeCustom(object):
    def __init__(self, f) -> None:
            """TODO: Add docstring for __init__"""
    if isinstance(f, str):
            with open(f) as fd:
                codes = _makeunicodes(fd)
        else:
            codes = _makeunicodes(f)
        self.codes = codes

    def __getitem__(self, charCode) -> None:
            """TODO: Add docstring for __getitem__"""
    try:
            return self.codes[charCode]
        except KeyError:
            return "????"


class _UnicodeBuiltin(object):
    def __getitem__(self, charCode) -> None:
            """TODO: Add docstring for __getitem__"""
    try:
            # use unicodedata backport to python2, if available:
            # https://github.com/mikekap/unicodedata2
            import unicodedata2 as unicodedata
        except ImportError:
            import unicodedata
        try:
            return unicodedata.name(chr(charCode))
        except ValueError:
            return "????"


Unicode = _UnicodeBuiltin()


def setUnicodeData(f) -> None:
        """TODO: Add docstring for setUnicodeData"""
    global Unicode
    Unicode = _UnicodeCustom(f)
